let latestHTML = '';
let menHTML = '';
let womenHTML = '';

products.forEach(product => {
  const productHTML = `
    <div class="product-container">
      <div class="product-image-container">
        <img class="product-image" src="${product.image}" />
      </div>
      <div class="product-name limit-text-to-2-lines">
        ${product.name}
      </div>
      <div class="product-rating-container">
        <img class="product-rating-stars" src="images/ratings/rating-${product.rating.stars * 10}.png" />
        <div class="product-rating-count link-primary">${product.rating.count}</div>
      </div>
      <div class="product-price">$${formatCurrency(product.priceCents)}</div>
      <div class="product-quantity-container">
        <select class='select-numbers'>
          ${Array.from({ length: 10 }, (_, i) => `<option value="${i + 1}">${i + 1}</option>`).join('')}
        </select>
      </div>
      <div class="product-spacer"></div>
      <div class="added-to-cart">
        <img src="images/icons/checkmark.png" />
        Added
      </div>
      <button class="add-to-cart-button button-primary js-add-to-cart"
        data-product-id="${product.id}">
        Add to Cart
      </button>
    </div>
  `;

  // Assign HTML based on category
  if (product.category === 'latest') latestHTML += productHTML;
  else if (product.category === 'men') menHTML += productHTML;
  else if (product.category === 'women') womenHTML += productHTML;
});

document.querySelector('.js-products-latest').innerHTML = latestHTML;
document.querySelector('.js-products-men').innerHTML = menHTML;
document.querySelector('.js-products-women').innerHTML = womenHTML;
